;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Give Me A Hand"
run/main_scene="res://scenes/dev_01.tscn"
config/icon="res://icon.png"

[display]

window/size/width=320
window/size/height=180
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="viewport"
window/stretch/aspect="keep_width"

[editor_plugins]

enabled=PoolStringArray( "res://addons/GodotLDtkImporter-c85b6e4d0ecfca0130a1c595c907a17730ca7c7e/plugin.cfg", "res://addons/LDtk-Importer/plugin.cfg", "res://addons/gut/plugin.cfg" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
2d/snapping/use_gpu_pixel_snap=true
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
